

(s/def ::right #(cljstr/includes? (.-type %) "Node"))
(s/def ::left  #(cljstr/includes? (.-type %) "Node"))
(s/def ::equation?  (s/keys :req [::left ::right]) )

(defrecord equation [left right])

(defn tex [{:keys [left right] :as equa}]
  (s/conform equa ::equation?)
  (str
   (.toTex left)
   "="
   (.toTex right)))
